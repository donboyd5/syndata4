// "AMT_em": {"2013": [50900.0, 79800.0, 39400.0, 50900.0, 79800.0]}
// Reducing by $10,000 gives us:
// "AMT_em": {"2013": [41900.0, 70800.0, 30400.0, 41900.0, 70800.0]}
//
{
    "policy": {
        "_AMT_em_single": {
